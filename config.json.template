{
    "circuit": {
        "domain": "circuitsandbox.net"
    },
    "linkify": [{
        "name": "Jira",
        "search": "(ANS-[0-9]{1,5}|AAC-[0-9]{1,5}|CRI-[0-9]{1,5})",
        "replace": "https://<jira-server>/browse/$1"
    },
    {
        "name": "Gerrit",
        "search": "([4-6][0-9]{4})",
        "replace": "https://<gerrit-server>/#/c/$1"
    }],
    "sdkLogLevel": "debug"
}
